	<parameter name="congregacaoId" class="java.lang.String"/>
	<parameter name="dataReferencia" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
(select count(p.id) from [secretary].[secretary].publicador p where p.congregacao_id =1 and p.situacao_id in (1,5)) as totalPub,
c.id as congregacaoId,
COUNT(p.descricao) as TOTAL,
p.descricao as pioneiro,
sc.data_referencia,
SUM(sc.livros) AS livros,
SUM(sc.folhetos_brochuras) as folhetos,
SUM(sc.horas) AS horas,
SUM(sc.revistas) AS revistas,
SUM(sc.publicacoes) as publicacoes,
SUM(sc.revisitas) AS revisitas,
SUM(sc.estudos) AS estudos
from
[secretary].[secretary].servico_campo sc
LEFT JOIN [secretary].[secretary].publicador pb on sc.publicador_id = pb.id
LEFT JOIN [secretary].[secretary].pioneiro p on sc.pioneiro_id = p.id
LEFT JOIN [secretary].[secretary].grupo gr on pb.grupo_id = gr.id
LEFT JOIN [secretary].[secretary].congregacao c on pb.congregacao_id = c.id
LEFT JOIN [secretary].[secretary].dianteira d on pb.dianteira_id = d.id
where
c.id = $P{congregacaoId} and sc.horas > 0 and
sc.data_referencia = $P{dataReferencia}
group by c.id, p.descricao, sc.data_referencia, p.id
order by
p.id]]>
	</queryString>
