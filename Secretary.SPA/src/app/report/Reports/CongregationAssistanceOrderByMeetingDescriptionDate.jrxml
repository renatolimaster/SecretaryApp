	<queryString>
		<![CDATA[SELECT secretary.congregacao.id,
	secretary.congregacao.nome,
	secretary.reuniao.id,
	secretary.reuniao.descricao,
	secretary.congregacao.id,
	secretary.congregacao.nome,
	secretary.reuniao.id,
	secretary.reuniao.descricao,
	secretary.assistenciareuniao.data_referencia,
	secretary.assistenciareuniao.semana1,
	secretary.assistenciareuniao.estrangeiros1,
	secretary.assistenciareuniao.semana2,
	secretary.assistenciareuniao.estrangeiros2,
	secretary.assistenciareuniao.semana3,
	secretary.assistenciareuniao.estrangeiros3,
	secretary.assistenciareuniao.semana4,
	secretary.assistenciareuniao.estrangeiros4,
	secretary.assistenciareuniao.semana5,
	secretary.assistenciareuniao.estrangeiros5,
	secretary.assistenciareuniao.total,
	secretary.assistenciareuniao.media,
	secretary.assistenciareuniao.id
FROM secretary.assistenciareuniao
	inner join secretary.reuniao ON
	 secretary.assistenciareuniao.reuniao_id = secretary.reuniao.id
	inner join secretary.congregacao ON
	 secretary.reuniao.congregacao_id = secretary.congregacao.id
WHERE
	 secretary.congregacao.id = 1
	 AND (
	 secretary.assistenciareuniao.data_referencia BETWEEN $P{dtaStart} AND $P{dtaEnd})
ORDER BY secretary.reuniao.descricao,
	secretary.assistenciareuniao.data_referencia DESC]]>
	</queryString>
